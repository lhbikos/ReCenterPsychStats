	pages = {7778--7783},
	file = {Chang et al. - 2019 - The mixed effects of online diversity training.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\RZWVYIPL\\Chang et al. - 2019 - The mixed effects of online diversity training.pdf:application/pdf},
}

@article{banakou_virtual_2016,
