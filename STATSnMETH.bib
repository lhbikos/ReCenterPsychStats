	keywords = {psychological well-being, Well Being, Faith, faith, Race and Ethnic Discrimination, Minority Groups, race, meaning, Social Justice, oppression, agency, ancestral pride, collective memory, Collective Memory, collective orientation, cultural relevance, Future, future orientation, History, hope, Hope, individual orientation, Individualism, Oppression, past orientation, people of color, Pride, Psychological Theories, purpose, Racial and Ethnic Relations, radical hope, Radical Movements, resistance},
