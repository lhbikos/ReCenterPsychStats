	keywords = {Racism, Test Construction, Blacks, Discrimination, critical consciousness, Racial and Ethnic Groups, Feminism, Trauma, Intervention, activism, Activism, anti-Black racism, Black Lives Matter, Critical Thinking, Prevention, racial trauma},
	pages = {1--16},
