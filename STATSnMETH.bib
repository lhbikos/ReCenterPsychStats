	keywords = {Humans, Psychology, Methodology, Mathematical Modeling, Mathematics, Statistical Analysis, Statistics, Biomedical Research, Data Interpretation, Experimental, Graduate Psychology Education, mathematical models, Models, NHST, null hypothesis significance testing, Null Hypothesis Testing, Psychological, Research Design, Sir Ronald Fisher, Statistical, statistical models, Statistical Significance, Statistics as Topic, Teaching, teaching methodology},
	pages = {1--12},
	file = {Rodgers - 2010 - The epistemology of mathematical and statistical m.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\JZ4W75MQ\\Rodgers - 2010 - The epistemology of mathematical and statistical m.pdf:application/pdf},
