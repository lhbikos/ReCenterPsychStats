	keywords = {Psychology Education, Graduate Psychology Education, Social Justice, social justice, Advocacy, critical consciousness, Clinical Psychology Graduate Training, Curriculum Development, doctoral training models, intergroup dialogue, Postgraduate Training, Scientist-Practitioner-Advocate},
