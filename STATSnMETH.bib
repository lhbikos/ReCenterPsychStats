	keywords = {Test Construction, Rating Scales, Test Validity, Human Sex Differences, Distress, Stress, Human Females, Sexism, Blacks, Oppression, racism, Racism, gendered racism, sexism, stress, gendered racial microaggressions, Aggressiveness},
	pages = {289--302},
