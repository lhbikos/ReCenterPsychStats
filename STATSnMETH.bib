	author = {Bikos, Lynette H. and Çiftçi, Ayşe and Güneri, Oya Yerin and Demir, Cennet Engin and Sümer, Zeynep Hatipoglu and Danielson, Sharrie and DeVries, Shelly and Bilgen, Wendy A.},
