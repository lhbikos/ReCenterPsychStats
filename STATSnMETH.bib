	keywords = {Test Construction, Test Reliability, Test Validity, Psychotherapy, Multiculturalism, measure development, Cross Cultural Test Adaptation, Exploratory Factor Analysis, Focus Group, Item Response Theory, Measurement Invariance, multicultural orientation, psychotherapy process, Rasch item response theory, Therapist Attitudes},
