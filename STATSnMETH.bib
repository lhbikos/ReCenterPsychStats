	journal = {American Psychologist},
	author = {Rodgers, Joseph Lee},
	month = jan,
	year = {2010},
	keywords = {Humans, Research Design, Methodology, Mathematical Modeling, Mathematics, Statistical Analysis, Statistics, Models, Psychological, Biomedical Research, Data Interpretation, Statistical, Graduate Psychology Education, mathematical models, Models, Statistical, NHST, null hypothesis significance testing, Null Hypothesis Testing, Psychology, Experimental, Sir Ronald Fisher, statistical models, Statistical Significance, Statistics as Topic, Teaching, teaching methodology},
	pages = {1--12},
	file = {EBSCO Full Text:C\:\\Users\\lhbikos\\Zotero\\storage\\SMSS8ZLX\\Rodgers - 2010 - The epistemology of mathematical and statistical m.pdf:application/pdf}
