	keywords = {Intention, Occupational Stress, Depression (Emotion), economic stress, economics, Economics, entrepreneurship, Entrepreneurship, Financial Strain, organizational behavior, Organizational Behavior, social ties, Withdrawal (Defense Mechanism), withdrawal intentions},
	pages = {789--810},
