	file = {Paluck et al. - 2021 - Prejudice Reduction Progress and Challenges.pdf:C\:\\Users\\lhbikos\\Zotero\\storage\\69LVQKZI\\Paluck et al. - 2021 - Prejudice Reduction Progress and Challenges.pdf:application/pdf},
}

@article{chang_mixed_2019,
	title = {The mixed effects of online diversity training},
	volume = {116},
	copyright = {Copyright © 2019 the Author(s). Published by PNAS.. http://creativecommons.org/licenses/by/4.0/This open access article is distributed under Creative Commons Attribution License 4.0 (CC BY).},
